{
  "name": "tecla5-site",
  "description": "Gatsby default starter with design",
  "version": "1.0.0",
  "author": "",
  "dependencies": {
    "gatsby": "^2.0.19",
    "gatsby-image": "^2.0.15",
    "gatsby-plugin-manifest": "^2.0.5",
    "gatsby-plugin-offline": "^2.0.11",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.7",
    "gatsby-source-filesystem": "^2.0.4",
    "gatsby-transformer-sharp": "^2.1.4",
    "gatsby-plugin-sass": "^2.0.1",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build && cp CNAME ./public/ && touch ./public/.nojekyll",
    "data": "npx js-yaml ./src/data/en.yaml > ./src/data/en.json && echo 'updated data yaml to json'",
    "develop": "npm run data && gatsby develop",
    "start": "npm run develop",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "npm run build && gh-pages -d public -b master",
    "deploy2": "npm run build && ./deploy.sh",
    "serve": "gatsby serve"
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "node-sass": "^4.9.3",
    "prettier": "^1.14.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
